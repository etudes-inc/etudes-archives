<project>
	<pomVersion>3</pomVersion>
	<extend>../../project.xml</extend>
	<name>etudes-archives-api</name>
	<groupId>etudes</groupId>
	<id>etudes-archives-api</id>
	<currentVersion>${archives.version}</currentVersion>
	<organization>
		<name>Etudes, Inc.</name>
		<url>http://etudes.org/archives</url>
	</organization>
	<inceptionYear>2009</inceptionYear>

	<properties>
		<!-- deploy as a jar -->
		<deploy.type>jar</deploy.type>
		<!-- deploy to "shared", "common" or "server" -->
		<deploy.target>shared</deploy.target>
	</properties>

	<dependencies>

		<dependency>
			<groupId>sakaiproject</groupId>
			<artifactId>sakai-entity-api</artifactId>
			<version>${sakai.version}</version>
		</dependency>

		<dependency>
			<groupId>sakaiproject</groupId>
			<artifactId>sakai-user-api</artifactId>
			<version>${sakai.version}</version>
		</dependency>

		<dependency>
			<groupId>sakaiproject</groupId>
			<artifactId>sakai-util-api</artifactId>
			<version>${sakai.version}</version>
		</dependency>

		<dependency>
			<groupId>etudes</groupId>
			<artifactId>etudes-ambrosia-api</artifactId>
			<version>${ambrosia.version}</version>
		</dependency>

		<dependency>
			<groupId>etudes</groupId>
			<artifactId>etudes-util-api</artifactId>
			<version>1.0</version>
		</dependency>

	</dependencies>

	<build>

		<sourceDirectory>src/java</sourceDirectory>

	</build>

</project>
